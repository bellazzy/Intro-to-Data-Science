   "source": [
    "import scipy.stats as stats\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import math\n",
    "from scipy.stats import gamma\n",
    "\n",
    "# support\n",
    "support = np.linspace(0.01, 6, 100)\n",
    "\n",
    "# likelihood\n",
    "\n",
    "likelihood = np.array([np.prod(stats.expon.pdf(Y,scale=1/s)) for s in support])\n",
    "\n",
    "# prior\n",
    "\n",
    "prior = np.array([np.prod(4*s*math.exp(-2*s)) for s in support])\n",
    "\n",
    "# posterior\n",
    "integrand = prior * likelihood\n",
    "normalize = np.trapz(integrand, support)\n",
    "post = likelihood*prior/normalize\n",
    "\n",
    "# plot\n",
    "fig, ax = plt.subplots(1,1)\n",
    "ax.plot(support, prior)\n",
    "ax.plot(support, post)\n",
    "ax.legend(labels=[\"prior\", \"posterior | data\"]);"
   ]
  },
