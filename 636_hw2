    "import numpy as np\n",
    "from sklearn.model_selection import train_test_split\n",
    "X, X_test, C, y_test =  train_test_split(NB.values[:,0],NB.values[:,1], test_size=4/5, random_state=436)\n",
    "# add your code here\n",
    "k_pred1 =  beta_naive_bayes().estimate(X, C).prediction(X_test)\n",
    "\n",
    "# compute the accuracy\n",
    "a = k_pred1-y_test\n",
    "np.count_nonzero(a)\n",
    "# compute the accuracy\n",
    "acc = 1 - np.count_nonzero(a)/len(a)  \n",
    "\n",
    "print('The accuracy on test set is {0:0.4f}'. format(acc))"
   ]
